    labels: {
        Dynamic(0): "D1",
        Dynamic(1): "D2",
        Dynamic(2): "D3",
        Dynamic(3): "D4",
        Dynamic(4): "D5",
        Dynamic(5): "D6",
        Dynamic(6): "D7",
        Dynamic(7): "D8",

        Dynamic(8): "CLK",

        Dynamic(9): "OUT",
    },
    tiles: {
